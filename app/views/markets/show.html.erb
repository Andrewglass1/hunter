<h1><%= @market.name %></h1>
<a href="#" id="placeme">Place me on map</a>

<% @market.merchants.map(&:zip).uniq[0..3].each do |zip| %>
  <p>
  <input type="checkbox" data-property-value="<%=zip%>" data-property-name="zip" class="showhide"checked/><a href="#" data-property-value="<%=zip%>" data-property-name="zip" class="bouncepins">   <%= zip %></a></p>
<% end %>

  <p><input type="checkbox" data-property-value="Groupon" data-property-name="provider" class="showhide"checked/><a href="#" data-property-value="Groupon" data-property-name="provider" class="bouncepins">  Groupon Deal</a></p>

  <p><input type="checkbox" data-property-value="livingsocial" data-property-name="provider" class="showhide"checked/>
  <a href="#" data-property-value="livingsocial" data-property-name="provider" class="bouncepins">  LivingSocial Deal</a>
  </p>

  <p><input type="checkbox" data-property-value="true" data-property-name="run_with_ls" class="showhide"checked/>
  <a href="#" data-property-value="true" data-property-name="run_with_ls" class="bouncepins">  LivingSocial Merchants</a>
  </p>


<div id='revenue-range'></div>
<p>
  <label for="filtered-rev">Filter by Revenue:</label>
  <input type="text" id="filtered-rev" style="border:0; color:#f6931f; font-weight:bold;" />
</p>

<div id='date-range'></div>
<p>
  <label for="filtered-dates">Filter by Date:</label>
  <input type="text" id="filtered-dates" style="border:0; color:#f6931f; font-weight:bold;" />
</p>

<script type="text/javascript">
  var Market = {
    min_revenue: <%= @market.min_revenue %>,
    max_revenue: <%= @market.max_revenue %>,
    max_days:  <%= @market.days_for_selector %>
  }
</script>

<%= gmaps(@gmap_options) %>